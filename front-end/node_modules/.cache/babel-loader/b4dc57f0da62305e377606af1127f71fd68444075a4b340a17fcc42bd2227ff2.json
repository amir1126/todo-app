{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    var showinp = false;\n    const updates = null;\n    store.dispatch('getData');\n    const handlerDelete = id => {\n      axios.delete(`http://localhost:4000/books/${id}`).then(res => {\n        store.dispatch('getData');\n      }).catch(err => console.log(err));\n    };\n    const handlerUpdate = id => {\n      this.showinp = true;\n      axios.patch(`http://localhost:4000/books/${id}`, {\n        Todo: this.updates\n      }).then(res => {\n        store.dispatch('getData');\n        this.$refs.inp.reset();\n      }).catch(err => console.log(res));\n    };\n    return {\n      store,\n      handlerDelete,\n      handlerUpdate,\n      showinp,\n      updates\n    };\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,IAAIM,OAAM,GAAI,KAAI;IAClB,MAAMC,OAAM,GAAI,IAAG;IACnBF,KAAK,CAACG,QAAQ,CAAC,SAAS;IACxB,MAAMC,aAAY,GAAKC,EAAE,IAAK;MAC1BX,KAAK,CAACY,MAAM,CAAE,+BAA8BD,EAAG,EAAC,EAC3CE,IAAI,CAAEC,GAAG,IAAK;QACXR,KAAK,CAACG,QAAQ,CAAC,SAAS;MAC5B,CAAC,EACAM,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC;IACA,MAAMG,aAAY,GAAKR,EAAE,IAAK;MAC1B,IAAI,CAACJ,OAAM,GAAI,IAAG;MAClBP,KAAK,CAACoB,KAAK,CAAE,+BAA8BT,EAAG,EAAC,EAAE;QAC7CU,IAAI,EAAE,IAAI,CAACb;MACf,CAAC,EACIK,IAAI,CAAEC,GAAG,IAAK;QACXR,KAAK,CAACG,QAAQ,CAAC,SAAS;QACxB,IAAI,CAACa,KAAK,CAACC,GAAG,CAACC,KAAK,EAAC;MACzB,CAAC,EACAT,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACJ,GAAG,CAAC;IACxC;IACA,OAAO;MACHR,KAAK;MACLI,aAAa;MACbS,aAAa;MACbZ,OAAO;MACPC;IACJ;EACJ;AACJ","names":["axios","useStore","workoutFormVue","name","components","data","store","showinp","updates","dispatch","handlerDelete","id","delete","then","res","catch","err","console","log","handlerUpdate","patch","Todo","$refs","inp","reset"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/navVue.vue"],"sourcesContent":["<template>\n    <div class=\"text-task\">\n        <h3>My Task:</h3>\n    </div>\n    <div class=\"between-flex\" v-for=\"workout in store.state.workouts\">\n        <div>\n            <span>{{ workout.Todo }}</span>\n        </div>\n        <div>\n            <button class=\"btn-del\" @click=\"handlerDelete(workout._id)\">Delete</button>\n            <button class=\"btn-upd\" @click=\"handlerUpdate(workout._id)\">Update</button>\n        </div>\n    </div><input ref=\"inp\" v-show=\"showinp\" v-model=\"updates\" type=\"text\" />\n</template>\n\n<script>\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    data() {\n        const store = useStore()\n        var showinp = false\n        const updates = null\n        store.dispatch('getData')\n        const handlerDelete = (id) => {\n            axios.delete(`http://localhost:4000/books/${id}`)\n                .then((res) => {\n                    store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        }\n        const handlerUpdate = (id) => {\n            this.showinp = true\n            axios.patch(`http://localhost:4000/books/${id}`, {\n                Todo: this.updates\n            })\n                .then((res) => {\n                    store.dispatch('getData')\n                    this.$refs.inp.reset()\n                })\n                .catch((err) => console.log(res))\n        }\n        return {\n            store,\n            handlerDelete,\n            handlerUpdate,\n            showinp,\n            updates\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}