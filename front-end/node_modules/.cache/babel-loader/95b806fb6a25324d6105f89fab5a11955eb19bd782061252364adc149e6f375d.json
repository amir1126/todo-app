{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'signin',\n  setup() {\n    const email = ref('');\n    const password = ref('');\n    const store = useStore();\n    const error = ref(false);\n    const isError = ref(false);\n    const submitSiginup = e => {\n      e.preventDefault();\n      axios.post('http://localhost:4000/signin', {\n        email: store.state.email,\n        password: store.state.password\n      }).then(res => {\n        store.state.result = res.data;\n        isError.value = false;\n        localStorage.setItem('user', JSON.stringify(res));\n        console.log(res.data.email);\n      }).catch(err => {\n        isError.value = true;\n        error.value = err.response.data.msg;\n        console.log(err.response.data.msg);\n      });\n    };\n    return {\n      email,\n      password,\n      error,\n      isError,\n      submitSiginup,\n      store\n    };\n  }\n};","map":{"version":3,"mappings":"AA8BA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,MAAMM,QAAO,GAAIN,GAAG,CAAC,EAAE;IACvB,MAAMO,KAAI,GAAIN,QAAQ,EAAC;IACvB,MAAMO,KAAI,GAAIR,GAAG,CAAC,KAAK;IACvB,MAAMS,OAAM,GAAIT,GAAG,CAAC,KAAK;IACzB,MAAMU,aAAY,GAAKC,CAAC,IAAK;MACzBA,CAAC,CAACC,cAAc,EAAE;MAClBV,KAAK,CAACW,IAAI,CAAC,8BAA8B,EAAE;QACvCR,KAAK,EAAEE,KAAK,CAACO,KAAK,CAACT,KAAK;QACxBC,QAAQ,EAAEC,KAAK,CAACO,KAAK,CAACR;MAC1B,CAAC,EACIS,IAAI,CAAEC,GAAG,IAAK;QACXT,KAAK,CAACO,KAAK,CAACG,MAAK,GAAID,GAAG,CAACE,IAAG;QAC5BT,OAAO,CAACU,KAAI,GAAI,KAAI;QACpBC,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACP,GAAG,CAAC;QAChDQ,OAAO,CAACC,GAAG,CAACT,GAAG,CAACE,IAAI,CAACb,KAAK,CAAC;MAC/B,CAAC,EACAqB,KAAK,CAAEC,GAAG,IAAK;QACZlB,OAAO,CAACU,KAAI,GAAI,IAAG;QACnBX,KAAK,CAACW,KAAI,GAAIQ,GAAG,CAACC,QAAQ,CAACV,IAAI,CAACW,GAAE;QAClCL,OAAO,CAACC,GAAG,CAACE,GAAG,CAACC,QAAQ,CAACV,IAAI,CAACW,GAAG;MACrC,CAAC;IACT;I