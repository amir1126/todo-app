       .catch((err) => console.log(err))\n        },\n        handlerUpdate: function (id) {\n            // this.showinp = true\n            if (this.updates === null) {\n                this.showinp = true\n            } if (this.updates !== null) {\n                axios.patch(`http://localhost:4000/books/${id}`, {\n                    Todo: this.updates\n                })\n                    .then((res) => {\n                        this.store.dispatch('getData')\n                        this.showinp = false\n                        this.updates = null\n                    })\n                    .catch((err) => console.log(err));\n            } else {\n                this.showinp = true\n            }\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}