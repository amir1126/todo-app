{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nimport { ref } from 'vue';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  setup() {\n    const store = useStore();\n    var showinp = ref(false);\n    const updates = ref(null);\n    store.dispatch('getData');\n    return {\n      store,\n      showinp,\n      updates\n    };\n  },\n  methods: {\n    handlerDelete: function (id) {\n      axios.delete(`http://localhost:4000/books/${id}`).then(res => {\n        this.store.dispatch('getData');\n      }).catch(err => console.log(err));\n    },\n    handlerUpdate: function (id) {\n      // this.showinp = true\n      if (this.updates === null) {\n        this.showinp = true;\n      }\n      if (this.updates !== null) {\n        axios.patch(`http://localhost:4000/books/${id}`, {\n          Todo: this.updates\n        }).then(res => {\n          this.store.dispatch('getData');\n          this.showinp = false;\n          this.updates = null;\n        }).catch(err => console.log(err));\n      } else {\n        this.showinp = true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,IAAIO,OAAM,GAAIN,GAAG,CAAC,KAAK;IACvB,MAAMO,OAAM,GAAIP,GAAG,CAAC,IAAI;IACxBK,KAAK,CAACG,QAAQ,CAAC,SAAS;IACxB,OAAO;MACHH,KAAK;MACLC,OAAO;MACPC;IACJ;EACJ,CAAC;EACDE,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzBb,KAAK,CAACc,MAAM,CAAE,+BAA8BD,EAAG,EAAC,EAC3CE,IAAI,CAAEC,GAAG,IAAK;QACX,IAAI,CAACT,KAAK,CAACG,QAAQ,CAAC,SAAS;MACjC,CAAC,EACAO,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC,CAAC;IACDG,aAAa,EAAE,UAAUR,EAAE,EAAE;MACzB;MACA,IAAI,IAAI,CAACJ,OAAM,KAAM,IAAI,EAAE;QACvB,IAAI,CAACD,OAAM,GAAI,IAAG;MACtB;MAAE,IAAI,IAAI,CAACC,OAAM,KAAM,IAAI,EAAE;Q