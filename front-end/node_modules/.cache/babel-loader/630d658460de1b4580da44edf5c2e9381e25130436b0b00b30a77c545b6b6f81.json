{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'workoutForm',\n  data() {\n    const title = '';\n    const family = '';\n    const number = '';\n    function onsubmit(e) {\n      e.preventDefault();\n      let workout = {};\n      axios.post('http://localhost:4000/books', {\n        methods: 'POST',\n        body: JSON.stringify(workout),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(res => {\n        workout = [title, family, number];\n        console.log(res);\n        console.log(workout);\n        console.log(number);\n      }).catch(err => console.log(err));\n    }\n    return {\n      onsubmit,\n      title,\n      family,\n      number\n    };\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAI,GAAG;IACH,MAAMC,KAAI,GAAI,EAAC;IACf,MAAMC,MAAK,GAAI,EAAC;IAChB,MAAMC,MAAK,GAAI,EAAC;IAChB,SAASC,QAAQ,CAACC,CAAC,EAAE;MACjBA,CAAC,CAACC,cAAc,EAAC;MACjB,IAAIC,OAAM,GAAI,CAAC;MAEfT,KAAK,CAACU,IAAI,CAAC,6BAA6B,EAAE;QACtCC,OAAO,EAAE,MAAM;QACfC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACL,OAAO,CAAC;QAC7BM,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MAEJ,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;QACXR,OAAM,GAAI,CAACN,KAAK,EAAEC,MAAM,EAAEC,MAAM;QAChCa,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;QAChBC,OAAO,CAACC,GAAG,CAACV,OAAO,CAAC;QACpBS,OAAO,CAACC,GAAG,CAACd,MAAM,CAAC;MACvB,CAAC,EACAe,KAAK,CAAEC,GAAG,IAAKH,OAAO,CAACC,GAAG,CAACE,GAAG,CAAC;IACxC;IACA,OAAO;MACHf,QAAQ;MACRH,KAAK;MACLC,MAAM;MACNC;IACJ;EACJ;AACJ","names":["axios","name","data","title","family","number","onsubmit","e","preventDefault","workout","post","methods","body","JSON","stringify","headers","then","res","console","log","catch","err"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/workoutForm.vue"],"sourcesContent":["<template>\n    <div>\n        <form :onSubmit=\"onsubmit\">\n            <label>Name:</label>\n            <input type=\"text\" :value=\"title\" @input=\"event => title = event.target.value\">\n            <label>Family:</label>\n            <input type=\"text\" :value=\"family\" @input=\"event => family = event.target.value\">\n            <label>Number:</label>\n            <input type=\"number\" :value=\"number\" @input=\"event => number = event.target.value\">\n            <h1>{{ number }}</h1>\n            <button>Submit</button>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n    name: 'workoutForm',\n    data() {\n        const title = ''\n        const family = ''\n        const number = ''\n        function onsubmit(e) {\n            e.preventDefault()\n            let workout = {}\n            \n            axios.post('http://localhost:4000/books', {\n                methods: 'POST',\n                body: JSON.stringify(workout),\n                headers: {\n                    'Content-Type': 'application/json'\n                },\n\n            })\n                .then((res) => {\n                    workout = [title, family, number]\n                    console.log(res);\n                    console.log(workout);\n                    console.log(number);\n                })\n                .catch((err) => console.log(err))\n        }\n        return {\n            onsubmit,\n            title,\n            family,\n            number\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}