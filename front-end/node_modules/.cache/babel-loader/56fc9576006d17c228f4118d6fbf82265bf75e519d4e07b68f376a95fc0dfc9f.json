{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    store.dispatch('getData');\n    const handlerDelete = id => {\n      axios.delete(`http://localhost:4000/books/${id}`).then(res => {\n        store.dispatch('getData');\n      }).catch(err => console.log(err));\n    };\n    const handlerUpdate = id => {\n      axios.patch(`http://localhost:4000/books/${id._id}`, {\n        title: store.state.title,\n        Body: store.state.Body,\n        Content: store.state.Content,\n        Code: store.state.Code\n      }).then(res => {\n        store.dispatch('getData');\n      }).catch(err => console.log(err));\n    };\n    return {\n      store,\n      handlerDelete,\n      handlerUpdate\n    };\n  }\n};","map":{"version":3,"mappings":"AAWA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvBK,KAAK,CAACC,QAAQ,CAAC,SAAS;IACxB,MAAMC,aAAY,GAAKC,EAAE,IAAK;MAC1BT,KAAK,CAACU,MAAM,CAAE,+BAA8BD,EAAG,EAAC,EAC3CE,IAAI,CAAEC,GAAG,IAAK;QACXN,KAAK,CAACC,QAAQ,CAAC,SAAS;MAC5B,CAAC,EACAM,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC;IACA,MAAMG,aAAY,GAAKR,EAAE,IAAK;MAC1BT,KAAK,CAACkB,KAAK,CAAE,+BAA8BT,EAAE,CAACU,GAAI,EAAC,EAC/C;QACIC,KAAK,EAAEd,KAAK,CAACe,KAAK,CAACD,KAAK;QACxBE,IAAI,EAAEhB,KAAK,CAACe,KAAK,CAACC,IAAI;QACtBC,OAAO,EAAEjB,KAAK,CAACe,KAAK,CAACE,OAAO;QAC5BC,IAAI,EAAElB,KAAK,CAACe,KAAK,CAACG;MAEtB,CAAC,EACAb,IAAI,CAAEC,GAAG,IAAK;QACXN,KAAK,CAACC,QAAQ,CAAC,SAAS;MAC5B,CAAC,EACAM,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC;IACA,OAAO;MACHR,KAAK;MACLE,aAAa;MACbS;IACJ;EACJ;AACJ","names":["axios","useStore","workoutFormVue","name","components","data","store","dispatch","handlerDelete","id","delete","then","res","catch","err","console","log","handlerUpdate","patch","_id","title","state","Body","Content","Code"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/navVue.vue"],"sourcesContent":["<template>\n    <div class=\"between-flex\" v-for=\"workout in store.state.workouts\">\n        <div>\n            <span>{{ workout.Todo }}</span>\n            <button @click=\"handlerDelete(workout._id)\">Delete</button>\n            <button @click=\"handlerUpdate(workout)\">Update</button>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    data() {\n        const store = useStore()\n        store.dispatch('getData')\n        const handlerDelete = (id) => {\n            axios.delete(`http://localhost:4000/books/${id}`)\n                .then((res) => {\n                    store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        }\n        const handlerUpdate = (id) => {\n            axios.patch(`http://localhost:4000/books/${id._id}`,\n                {\n                    title: store.state.title,\n                    Body: store.state.Body,\n                    Content: store.state.Content,\n                    Code: store.state.Code\n\n                })\n                .then((res) => {\n                    store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        }\n        return {\n            store,\n            handlerDelete,\n            handlerUpdate\n        }\n    }\n}\n</script>\n\n<style>\nhr {\n    color: slateblue;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}