{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    const showinp = false;\n    const updates = null;\n    const getItem = localStorage.getItem(JSON.stringify('todo'));\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    if (localStorage.getItem('user')) {\n      // localStorage.getItem('todo')\n      // axios.get('/books', {\n      //     headers: { 'Authorization': `Bearer ${localStorage.getItem('token')}` },\n      // })\n      //     .then(response => {\n      //         this.store.state.workouts = response.data\n      //     })\n    }\n    return {\n      store,\n      showinp,\n      updates,\n      getItem\n      // timecreate\n    };\n  },\n\n  methods: {\n    handlerDelete: function (id) {\n      if (localStorage.getItem('user')) {\n        axios.delete(`/books/${id}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        }).then(res => {\n          axios.get('/books', {\n            headers: {\n              'Authorization': `Bearer ${localStorage.getItem('token')}`\n            }\n          }).then(response => {\n            this.store.state.workouts = response.data;\n          });\n        }).catch(err => console.log(err));\n      }\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (localStorage.getItem('user')) {\n        if (this.updates !== null) {\n          axios.patch(`/books/${id}`, {\n            Todo: this.updates,\n            headers: {\n              'Authorization': `Bearer ${this.store.state.result}`\n            }\n          }).then(res => {\n            axios.get('/books').then(response => {\n              this.store.state.workouts = response.data;\n            });\n            this.showinp = false;\n            this.updates = null;\n          }).catch(err => console.log(err));\n        }\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAI,KAAI;IACpB,MAAMC,OAAM,GAAI,IAAG;IACnB,MAAMC,OAAM,GAAIC,YAAY,CAACD,OAAO,CAACE,IAAI,CAACC,SAAS,CAAC,MAAM,CAAC;IAC3D;IACA,IAAIF,YAAY,CAACD,OAAO,CAAC,MAAM,CAAC,EAAE;MAC9B;MACA;MACA;MACA;MACA;MACA;MACA;IAAA;IAEJ,OAAO;MACHH,KAAK;MACLC,OAAO;MACPC,OAAO;MACPC;MACA;IACJ;EACJ,CAAC;;EACDI,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzB,IAAIL,YAAY,CAACD,OAAO,CAAC,MAAM,CAAC,EAAE;QAE9BT,KAAK,CAACgB,MAAM,CAAE,UAASD,EAAG,EAAC,EAAE;UACzBE,OAAO,EAAE;YAAE,eAAe,EAAG,UAASP,YAAY,CAACD,OAAO,CAAC,OAAO,CAAE;UAAE;QAC1E,CAAC,EACIS,IAAI,CAAEC,GAAG,IAAK;UACXnB,KAAK,CAACoB,GAAG,CAAC,QAAQ,EAAE;YAChBH,OAAO,EAAE;cAAE,eAAe,EAAG,UAASP,YAAY,CAACD,OAAO,CAAC,OAAO,CAAE;YAAE;UAC1E,CAAC,EACIS,IAAI,CAACG,QAAO,IAAK;YACd,IAAI,CAACf,KAAK,CAACgB,KAAK,CAACC,QAAO,GAAIF,QAAQ,CAAChB,IAAG;UAC5C,CAAC;QACT,CAAC,EACAmB,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MACxC;IACJ,CAAC;IACDG,aAAa,EAAE,UAAUb,EAAE,EAAE;MACzB,IAAI,CAACR,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;MAC3B,IAAIG,YAAY,CAACD,OAAO,CAAC,MAAM,CAAC,EAAE;QAE9B,IAAI,IAAI,CAACD,OAAM,KAAM,IAAI,EAAE;UACvBR,KAAK,CAAC6B,KAAK,CAAE,UAASd,EAAG,EAAC,EAAE;YACxBe,IAAI,EAAE,IAAI,CAACtB,OAAO;YAClBS,OAAO,EAAE;cAAE,eAAe,EAAG,UAAS,IAAI,CAACX,KAAK,CAACgB,KAAK,CAACS,MAAO;YAAE;UAEpE,CAAC,EACIb,IAAI,CAAEC,GAAG,IAAK;YACXnB,KAAK,CAACoB,GAAG,CAAC,QAAQ,EACbF,IAAI,CAACG,Q