{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    const showinp = false;\n    const updates = null;\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    if (localStorage.getItem('user')) {\n      JSON.parse(localStorage.setItem('todo', store.state.Todo));\n      // axios.get('/books', {\n      //     headers: { 'Authorization': `Bearer ${localStorage.getItem('token')}` },\n      // })\n      //     .then(response => {\n      //         this.store.state.workouts = response.data\n      //     })\n    }\n\n    return {\n      store,\n      showinp,\n      updates\n      // timecreate\n    };\n  },\n\n  methods: {\n    handlerDelete: function (id) {\n      if (localStorage.getItem('user')) {\n        axios.delete(`/books/${id}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        }).then(res => {\n          axios.get('/books', {\n            headers: {\n              'Authorization': `Bearer ${localStorage.getItem('token')}`\n            }\n          }).then(response => {\n            this.store.state.workouts = response.data;\n          });\n        }).catch(err => console.log(err));\n      }\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (localStorage.getItem('user')) {\n        if (this.updates !== null) {\n          axios.patch(`/books/${id}`, {\n            Todo: this.updates,\n            headers: {\n              'Authorization': `Bearer ${this.store.state.result}`\n            }\n          }).then(res => {\n            axios.get('/books').then(response => {\n              this.store.state.workouts = response.data;\n            });\n            this.showinp = false;\n            this.updates = null;\n          }).catch(err => console.log(err));\n        }\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAI,KAAI;IACpB,MAAMC,OAAM,GAAI,IAAG;IACnB;IACA,IAAIC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,EAAE;MAC9BC,IAAI,CAACC,KAAK,CAACH,YAAY,CAACI,OAAO,CAAC,MAAM,EAAEP,KAAK,CAACQ,KAAK,CAACC,IAAI,CAAC;MACzD;MACA;MACA;MACA;MACA;MACA;IACJ;;IACA,OAAO;MACHT,KAAK;MACLC,OAAO;MACPC;MACA;IACJ;EACJ,CAAC;;EACDQ,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzB,IAAIT,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,EAAE;QAE9BV,KAAK,CAACmB,MAAM,CAAE,UAASD,EAAG,EAAC,EAAE;UACzBE,OAAO,EAAE;YAAE,eAAe,EAAG,UAASX,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;UAAE;QAC1E,CAAC,EACIW,IAAI,CAAEC,GAAG,IAAK;UACXtB,KAAK,CAACuB,GAAG,CAAC,QAAQ,EAAE;YAChBH,OAAO,EAAE;cAAE,eAAe,EAAG,UAASX,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;YAAE;UAC1E,CAAC,EACIW,IAAI,CAACG,QAAO,IAAK;YACd,IAAI,CAAClB,KAAK,CAACQ,KAAK,CAACW,QAAO,GAAID,QAAQ,CAACnB,IAAG;UAC5C,CAAC;QACT,CAAC,EACAqB,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MACxC;IACJ,CAAC;IACDG,aAAa,EAAE,UAAUZ,EAAE,EAAE;MACzB,IAAI,CAACX,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;MAC3B,IAAIE,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,EAAE;QAE9B,IAAI,IAAI,CAACF,OAAM,KAAM,IAAI,EAAE;UACvBR,KAAK,CAAC+B,KAAK,CAAE,UAASb,EAAG,EAAC,EAAE;YACxBH,IAAI,EAAE,IAAI,CAACP,OAAO;YAClBY,OAAO,EAAE;cAAE,eAAe,EAAG,UAAS,IAAI,CAACd,KAAK,CAACQ,KAAK,CAACkB,MAAO;YAAE;UAEpE,CAAC,EACIX,IAAI,CAAEC,GAAG,IAAK;YACXtB,KAAK,CAACuB,GAAG,CAAC,QAAQ,EACbF,IAAI,CAACG,QAAO,IAAK;cACd,IAAI,CAAClB,KAAK,CAACQ,KAAK,CAACW,QAAO