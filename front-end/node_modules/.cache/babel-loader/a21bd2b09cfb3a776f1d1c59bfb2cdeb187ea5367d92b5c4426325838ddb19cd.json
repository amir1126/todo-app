{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    const showinp = false;\n    const updates = null;\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    return {\n      store,\n      showinp,\n      updates\n      // timecreate\n    };\n  },\n\n  created() {\n    if (localStorage.getItem('user')) {\n      axios.get('/books', {\n        headers: {\n          'Authorization': `Bearer ${localStorage.getItem('token')}`\n        }\n      }).then(response => {\n        this.store.state.workouts = response.data;\n      });\n    }\n  },\n  methods: {\n    handlerDelete: function (id) {\n      if (localStorage.getItem('token')) {\n        axios.delete(`/books/${id}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        }).then(res => {\n          axios.get('/books', {\n            headers: {\n              'Authorization': `Bearer ${localStorage.getItem('token')}`\n            }\n          }).then(response => {\n            this.store.state.workouts = response.data;\n          });\n        }).catch(err => console.log(err));\n      }\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (localStorage.getItem('token')) {\n        if (this.updates !== null) {\n          axios.patch(`/books/${id}`, {\n            Todo: this.updates,\n            headers: {\n              'Authorization': `Bearer ${this.store.state.result}`\n            }\n          }).then(res => {\n            axios.get('/books').then(response => {\n              this.store.state.workouts = response.data;\n            });\n            this.showinp = false;\n            this.updates = null;\n          }).catch(err => console.log(err));\n        }\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAI,KAAI;IACpB,MAAMC,OAAM,GAAI,IAAG;IACnB;IACA,OAAO;MACHF,KAAK;MACLC,OAAO;MACPC;MACA;IACJ;EACJ,CAAC;;EACDC,OAAO,GAAG;IACN,IAAIC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,EAAE;MAC9BX,KAAK,CAACY,GAAG,CAAC,QAAQ,EAAE;QAChBC,OAAO,EAAE;UAAE,eAAe,EAAG,UAASH,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;QAAE;MAC1E,CAAC,EACIG,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACT,KAAK,CAACU,KAAK,CAACC,QAAO,GAAIF,QAAQ,CAACV,IAAG;MAC5C,CAAC;IACT;EACJ,CAAC;EACDa,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzB,IAAIV,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;QAE/BX,KAAK,CAACqB,MAAM,CAAE,UAASD,EAAG,EAAC,EAAE;UACzBP,OAAO,EAAE;YAAE,eAAe,EAAG,UAASH,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;UAAE;QAC1E,CAAC,EACIG,IAAI,CAAEQ,GAAG,IAAK;UACXtB,KAAK,CAACY,GAAG,CAAC,QAAQ,EAAE;YAChBC,OAAO,EAAE;cAAE,eAAe,EAAG,UAASH,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;YAAE;UAC1E,CAAC,EACIG,IAAI,CAACC,QAAO,IAAK;YACd,IAAI,CAACT,KAAK,CAACU,KAAK,CAACC,QAAO,GAAIF,QAAQ,CAACV,IAAG;UAC5C,CAAC;QACT,CAAC,EACAkB,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MACxC;IACJ,CAAC;IACDG,aAAa,EAAE,UAAUP,EAAE,EAAE;MACzB,IAAI,CAACb,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;MAC3B,IAAIG,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;QAE/B,IAAI,IAAI,CAACH,OAAM,KAAM,IAAI,EAAE;UACvBR,KAAK,CAAC4B,KAAK,CAAE,UAASR,EAAG,EAAC,EAAE;YACxBS,IAAI,EAAE,IAAI,CAACrB,OAAO;YAClBK,OAAO,EAAE;cAAE,eAAe,EAAG,UAAS,IAAI,CAACP,KAAK,CAACU,KAAK,CAACc,MAAO;YAAE;UAEpE,CAAC,EACIhB,IAAI,CAAEQ,GAAG,IAAK;YACXtB,KAAK,CAACY,GAAG,CAAC,QAAQ,EACbE,IAAI,CA