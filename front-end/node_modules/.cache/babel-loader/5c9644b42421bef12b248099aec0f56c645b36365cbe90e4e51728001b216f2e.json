{"ast":null,"code":"import { useStore } from 'vuex';\nexport default {\n  name: 'login',\n  setup() {\n    const store = useStore();\n    const useAuth = e => {\n      e.preventDefault();\n      store.dispatch('useAuth');\n    };\n    return {\n      useAuth,\n      store\n    };\n  }\n};","map":{"version":3,"mappings":"AAuBA,SAASA,QAAO,QAAS,MAAM;AAC/B,eAAe;EACXC,IAAI,EAAE,OAAO;EACbC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIH,QAAQ,EAAC;IACvB,MAAMI,OAAM,GAAKC,CAAC,IAAK;MACnBA,CAAC,CAACC,cAAc,EAAE;MAClBH,KAAK,CAACI,QAAQ,CAAC,SAAS;IAC5B;IACA,OAAO;MACHH,OAAO;MACPD;IACJ;EACJ;AACJ","names":["useStore","name","setup","store","useAuth","e","preventDefault","dispatch"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/login.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit=\"useLogin\">\n        <div class=\"container\">\n            <div>\n                <h1>Log in</h1>\n                <p>Please fill in this form to create an account.</p>\n                <hr>\n\n                <label for=\"email\"><b>Email</b></label>\n                <input class=\"input\" v-model=\"store.state.email\" type=\"text\" placeholder=\"Enter Email\" name=\"email\" required>\n\n                <label for=\"psw\"><b>Password</b></label>\n                <input class=\"input\" v-model=\"store.state.password\" type=\"password\" placeholder=\"Enter Password\" name=\"psw\" required>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"signupbtn\">Log in</button>\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport { useStore } from 'vuex';\nexport default {\n    name: 'login',\n    setup() {\n        const store = useStore()\n        const useAuth = (e) => {\n            e.preventDefault();\n            store.dispatch('useAuth')\n        }\n        return {\n            useAuth,\n            store\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}