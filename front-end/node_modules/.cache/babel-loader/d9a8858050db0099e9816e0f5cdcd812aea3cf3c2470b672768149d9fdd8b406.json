ort default {\n    name: 'login',\n    setup() {\n        const store = useStore()\n        const useAuth = (e) => {\n            e.preventDefault();\n            store.dispatch('useLogin')\n            \n        }\n        \n        return {\n            useAuth,\n            store,\n            user\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}