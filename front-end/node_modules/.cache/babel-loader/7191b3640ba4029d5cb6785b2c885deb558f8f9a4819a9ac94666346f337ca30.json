{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    const showinp = false;\n    const updates = null;\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    return {\n      store,\n      showinp,\n      updates\n      // timecreate\n    };\n  },\n\n  async created() {\n    if (localStorage.getItem('token')) {\n      await axios.get('/books').then(response => {\n        console.log(response);\n        this.store.state.workouts = response.data;\n      });\n    }\n  },\n  methods: {\n    handlerDelete: function (id) {\n      if (localStorage.getItem('token')) {\n        axios.delete(`/books/${id}`, {\n          // headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n        }).then(res => {\n          axios.get('/books').then(response => {\n            this.store.state.workouts = response.data;\n          });\n        }).catch(err => console.log(err));\n      }\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (this.updates !== null) {\n        axios.patch(`/books/${id}`, {\n          Todo: this.updates\n          // headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n        }).then(res => {\n          axios.get('/books').then(response => {\n            this.store.state.workouts = response.data;\n          });\n          this.showinp = false;\n          this.updates = null;\n        }).catch(err => console.log(err));\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAI,KAAI;IACpB,MAAMC,OAAM,GAAI,IAAG;IACnB;IACA,OAAO;MACHF,KAAK;MACLC,OAAO;MACPC;MACA;IACJ;EACJ,CAAC;;EACD,MAAMC,OAAO,GAAG;IACZ,IAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;MAC/B,MAAMX,KAAK,CAACY,GAAG,CAAC,QAAQ,EACnBC,IAAI,CAACC,QAAO,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,IAAI,CAACR,KAAK,CAACW,KAAK,CAACC,QAAO,GAAIJ,QAAQ,CAACT,IAAG;MAC5C,CAAC;IACT;EACJ,CAAC;EACDc,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzB,IAAIX,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;QAE/BX,KAAK,CAACsB,MAAM,CAAE,UAASD,EAAG,EAAC,EAAE;UACzB;QAAA,CACH,EACIR,IAAI,CAAEU,GAAG,IAAK;UACXvB,KAAK,CAACY,GAAG,CAAC,QAAQ,EACbC,IAAI,CAACC,QAAO,IAAK;YACd,IAAI,CAACR,KAAK,CAACW,KAAK,CAACC,QAAO,GAAIJ,QAAQ,CAACT,IAAG;UAC5C,CAAC;QACT,CAAC,EACAmB,KAAK,CAAEC,GAAG,IAAKV,OAAO,CAACC,GAAG,CAACS,GAAG,CAAC;MACxC;IACJ,CAAC;IACDC,aAAa,EAAE,UAAUL,EAAE,EAAE;MACzB,IAAI,CAACd,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;MAC3B,IAAI,IAAI,CAACC,OAAM,KAAM,IAAI,EAAE;QACvBR,KAAK,CAAC2B,KAAK,CAAE,UAASN,EAAG,EAAC,EAAE;UACxBO,IAAI,EAAE,IAAI,CAACpB;UACX;QAEJ,CAAC,EACIK,IAAI,CAAEU,GAAG,IAAK;UACXvB,KAAK,CAACY,GAAG,CAAC,QAAQ,EACbC,IAAI,CAACC,QAAO,IAAK;YACd,IAAI,CAACR,KAAK,CAACW,KAAK,CAACC,QAAO,GAAIJ,QAAQ,CAACT,IAAG;UAC5C,CAAC;UACL,IAAI,CAACE,OAAM,GAAI,KAAI;UACnB,IAAI,CAACC,OAAM,GAAI,IAAG;QACtB,CAAC,EACAgB,KAAK,CAAEC,GAAG,IAAKV,OAAO,CAACC,GAAG,CAACS,GAAG,CAAC,CAAC;MACzC;IACJ,CAAC;IACDI,SAAS,EAAE,YAAY;MACnB,OAAO,IAAI,CAACtB,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;IACtC,CAAC;IACDuB,MAAM,EAAE,YAAY;MAChB,MAAMC,IAAG,GAAIC,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC;MAC5C,KAAK,IAAIC,IAAI,CAAC,EAAEA,IAAIH,IAAI,CAACI,MAAM,EAAE,EAAED,CAAC,EAAE;QAClCH,IAAI,CAACG,CAAC,CAAC,CAACE,SAAS,CAACC,MAAM,CAAC,SAAS,CAAC;MACvC;IACJ;EACJ;AACJ","names":["axios","useStore","workoutFormVue","name","components","data","store","showinp","updates","created","localStorage","getItem","get","then","response","console","log","state","workouts","methods","handlerDelete","id","delete","res","catch","err","handlerUpdate","patch","Todo","showinput","middle","list","document","getElementById","i","length","classList","toggle"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/Home.vue"],"sourcesContent":["<template>\n    <div class=\"text-task\">\n        <h3>My Task:</h3>\n    </div>\n    <div class=\"between-flex\" v-for=\"items in store.state.workouts\">\n        <div class=\"flex\">\n            <input @click=\"middle\" id=\"myCheck\" type=\"checkbox\">\n            <p id=\"todo\">{{ items.Todo }}</p>\n        </div>\n        <div>\n            <button class=\"btn-del\" @click=\"handlerDelete(items._id)\">Delete</button>\n            <button class=\"btn-upd\" @click=\"handlerUpdate(items._id), showinput\">Update</button>\n        </div>\n    </div>\n    <input v-if=\"showinp\" ref=\"inp\" v-model=\"updates\" type=\"text\" />\n</template>\n\n<script>\n// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    data() {\n        const store = useStore()\n        const showinp = false\n        const updates = null\n        // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n        return {\n            store,\n            showinp,\n            updates\n            // timecreate\n        }\n    },\n    async created() {\n        if (localStorage.getItem('token')) {\n            await axios.get('/books')\n                .then(response => {\n                    console.log(response);\n                    this.store.state.workouts = response.data\n                })\n        }\n    },\n    methods: {\n        handlerDelete: function (id) {\n            if (localStorage.getItem('token')) {\n\n                axios.delete(`/books/${id}`, {\n                    // headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n                })\n                    .then((res) => {\n                        axios.get('/books')\n                            .then(response => {\n                                this.store.state.workouts = response.data\n                            })\n                    })\n                    .catch((err) => console.log(err))\n            }\n        },\n        handlerUpdate: function (id) {\n            this.showinp = !this.showinp\n            if (this.updates !== null) {\n                axios.patch(`/books/${id}`, {\n                    Todo: this.updates,\n                    // headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n\n                })\n                    .then((res) => {\n                        axios.get('/books')\n                            .then(response => {\n                                this.store.state.workouts = response.data\n                            })\n                        this.showinp = false\n                        this.updates = null\n                    })\n                    .catch((err) => console.log(err));\n            }\n        },\n        showinput: function () {\n            return this.showinp = !this.showinp\n        },\n        middle: function () {\n            const list = document.getElementById(\"todo\");\n            for (var i = 0; i < list.length; ++i) {\n                list[i].classList.toggle('checked');\n            }\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n\n.flex {\n    display: flex;\n}\n\nspan {\n    font-size: 11px;\n    color: black;\n}\n\ninput[type=checkbox]:checked+p {\n    text-decoration: line-through;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}