{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'login',\n  data() {\n    const store = useStore();\n    return {\n      email: '',\n      password: '',\n      store\n    };\n  },\n  methods: {\n    authlogin: async function () {\n      await axios.post('/login', {\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        this.store.state.user = response.data.email;\n        localStorage.setItem('user', response.data.email);\n        this.$router.push('/');\n      }).catch(response => {\n        this.store.state.error = response.response.data.msg;\n        this.store.state.error = response.response.data.msg;\n        this.store.state.isError = true;\n        console.log(response.response);\n      });\n    },\n    showpass: function () {\n      const inp = document.getElementById('password');\n      if (inp.type === \"password\") {\n        inp.type = \"text\";\n      } else {\n        inp.type = \"password\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAI,EAAE,OAAO;EACbC,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIH,QAAQ,EAAC;IACvB,OAAO;MACHI,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZF;IACJ;EACJ,CAAC;EACDG,OAAO,EAAE;IACLC,SAAS,EAAE,kBAAkB;MACzB,MAAMR,KAAK,CAACS,IAAI,CAAC,QAAQ,EAAE;QACvBJ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACnB,CAAC,EACII,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACP,KAAK,CAACQ,KAAK,CAACC,IAAG,GAAIF,QAAQ,CAACR,IAAI,CAACE,KAAI;QAC1CS,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEJ,QAAQ,CAACR,IAAI,CAACE,KAAK;QAChD,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC,GAAG;MACzB,CAAC,EACAC,KAAK,CAACP,QAAO,IAAK;QACf,IAAI,CAACP,KAAK,CAACQ,KAAK,CAACO,KAAI,GAAIR,QAAQ,CAACA,QAAQ,CAACR,IAAI,CAACiB,GAAE;QAClD,IAAI,CAAChB,KAAK,CAACQ,KAAK,CAACO,KAAI,GAAIR,QAAQ,CAACA,QAAQ,CAACR,IAAI,CAACiB,GAAE;QAClD,IAAI,CAAChB,KAAK,CAACQ,KAAK,CAACS,OAAM,GAAI,IAAG;QAC9BC,OAAO,CAACC,GAAG,CAACZ,QAAQ,CAACA,QAAQ,CAAC;MAClC,CAAC;IACT,CAAC;IACDa,QAAQ,EAAE,YAAY;MAClB,MAAMC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU;MAC9C,IAAIF,GAAG,CAACG,IAAG,KAAM,UAAU,EAAE;QACzBH,GAAG,CAACG,IAAG,GAAI,MAAK;MACpB,OACK;QACDH,GAAG,CAACG,IAAG,GAAI,UAAS;MACxB;IACJ;EACJ;AACJ","names":["axios","useStore","name","data","store","email","password","methods","authlogin","post","then","response","state","user","localStorage","setItem","$router","push","catch","error","msg","isError","console","log","showpass","inp","document","getElementById","type"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/login.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit.prevent=\"authlogin\">\n        <div class=\"container\">\n            <div>\n                <h1>Log in</h1>\n                <p>Please fill in this form to create an account.</p>\n                <hr>\n\n                <label for=\"email\"><b>Email</b></label>\n                <input class=\"input\" v-model=\"email\" type=\"text\" placeholder=\"Enter Email\" name=\"email\" required>\n\n                <label for=\"psw\"><b>Password</b></label>\n                <input class=\"input\" v-model=\"password\" type=\"password\" id=\"password\" placeholder=\"Enter Password\" name=\"psw\"\n                    required>\n\n                <div>\n                    <input @click=\"showpass\" type=\"checkbox\">\n                    <p class=\"text\">show password</p>\n                </div>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"signupbtn\">Log in</button>\n                </div>\n                <div v-if=\"store.state.isError\" class=\"error\">\n                    {{ store.state.error }}\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { useStore } from 'vuex'\nexport default {\n    name: 'login',\n    data() {\n        const store = useStore()\n        return {\n            email: '',\n            password: '',\n            store\n        }\n    },\n    methods: {\n        authlogin: async function () {\n            await axios.post('/login', {\n                email: this.email,\n                password: this.password\n            })\n                .then(response => {\n                    this.store.state.user = response.data.email\n                    localStorage.setItem('user', response.data.email)\n                    this.$router.push('/')\n                })\n                .catch(response => {\n                    this.store.state.error = response.response.data.msg\n                    this.store.state.error = response.response.data.msg\n                    this.store.state.isError = true\n                    console.log(response.response);\n                })\n        },\n        showpass: function () {\n            const inp = document.getElementById('password')\n            if (inp.type === \"password\") {\n                inp.type = \"text\"\n            }\n            else {\n                inp.type = \"password\"\n            }\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}