{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    return {\n      store\n      // timecreate\n    };\n  },\n\n  async created() {\n    if (localStorage.getItem('token')) {\n      await axios.get('/books').then(response => {\n        console.log(response);\n        this.store.state.workouts = response.data;\n      });\n    }\n  },\n  methods: {\n    handlerDelete: function (id) {\n      axios.delete(`/books/${id}`, {\n        // headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n      }).then(res => {\n        axios.get('/books').then(response => {\n          console.log(response);\n          this.store.state.workouts = response.data;\n        });\n      }).catch(err => console.log(err));\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (this.updates !== null) {\n        axios.patch(`/books/${id}`, {\n          Todo: this.updates\n          // headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n        }).then(res => {\n          this.store.dispatch('getData');\n          this.showinp = false;\n          this.updates = null;\n        }).catch(err => console.log(err));\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;