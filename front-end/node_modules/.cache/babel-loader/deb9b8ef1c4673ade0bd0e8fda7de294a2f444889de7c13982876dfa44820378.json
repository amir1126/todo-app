    if (!this.store.state.user) {\n                return\n            }\n            const response = await fetch('/api/books', {\n                // mode: 'cors',\n                // headers: { 'Authorization': `Bearer ${JSON.parse(localStorage.getItem('user'))}` }\n            })\n            const json = await response.json()\n\n            if (response.ok) {\n                console.log(state.user);\n                this.store.state.workouts = json\n            }\n        } if (store.state.user) {\n            getData()\n        }\n        // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n        return {\n            store,\n            showinp,\n            updates,\n            names\n            // timecreate\n        }\n    },\n    methods: {\n\n        handlerDelete: function (id) {\n            axios.delete(`http://localhost:3000/books/${id}`, {\n                headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n            })\n                .then((res) => {\n                    this.store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        },\n        handlerUpdate: function (id) {\n            this.showinp = !this.showinp\n            if (this.updates !== null) {\n                axios.patch(`http://localhost:3000/books/${id}`, {\n                    Todo: this.updates,\n                    headers: { 'Authorization': `Bearer ${this.store.state.result}` },\n\n                })\n                    .then((res) => {\n                        this.store.dispatch('getData')\n                        this.showinp = false\n                        this.updates = null\n                    })\n                    .catch((err) => console.log(err));\n            }\n        },\n        showinput: function () {\n            return this.showinp = !this.showinp\n        },\n        middle: function () {\n            const list = document.getElementById(\"todo\");\n            for (var i = 0; i < list.length; ++i) {\n                list[i].classList.toggle('checked');\n            }\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n\n.flex {\n    display: flex;\n}\n\nspan {\n    font-size: 11px;\n    color: black;\n}\n\ninput[type=checkbox]:checked+p {\n    text-decoration: line-through;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}