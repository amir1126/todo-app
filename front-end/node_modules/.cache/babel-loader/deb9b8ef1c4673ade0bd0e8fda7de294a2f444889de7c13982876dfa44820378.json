{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport { ref } from 'vue';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  setup() {\n    const store = useStore();\n    var showinp = ref(false);\n    const updates = ref(null);\n    const names = ref('');\n    async function getData() {\n      if (!this.store.state.user) {\n        return;\n      }\n      const response = await fetch('/api/books', {\n        // mode: 'cors',\n        // headers: { 'Authorization': `Bearer ${JSON.parse(localStorage.getItem('user'))}` }\n      });\n      const json = await response.json();\n      if (response.ok) {\n        console.log(state.user);\n        this.store.state.workouts = json;\n      }\n    }\n    if (store.state.user) {\n      getData();\n    }\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    return {\n      store,\n      showinp,\n      updates,\n      names\n      // timecreate\n    };\n  },\n\n  methods: {\n    handlerDelete: function (id) {\n      axios.delete(`http://localhost:3000/books/${id}`, {\n        headers: {\n          'Authorization': `Bearer ${this.store.state.result}`\n        }\n      }).then(res => {\n        this.store.dispatch('getData');\n      }).catch(err => console.log(err));\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (this.updates !== null) {\n        axios.patch(`http://localhost:3000/books/${id}`, {\n          Todo: this.updates,\n          headers: {\n            'Authorization': `Bearer ${this.store.state.result}`\n          }\n        }).then(res => {\n          this.store.dispatch('getData');\n          this.showinp = false;\n          this.updates = null;\n        }).catch(err => console.log(err));\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,IAAIO,OAAM,GAAIN,GAAG,CAAC,KAAK;IACvB,MAAMO,OAAM,GAAIP,GAAG,CAAC,IAAI;IACxB,MAAMQ,KAAI,GAAIR,GAAG,CAAC,EAAE;IACpB,eAAeS,OAAO,GAAG;MACrB,IAAI,CAAC,IAAI,CAACJ,KAAK,CAACK,KAAK,CAACC,IAAI,EAAE;QACxB;MACJ;MACA,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,YAAY,EAAE;QACvC;QACA;MAAA,CACH;MACD,MAAMC,IAAG,GAAI,MAAMF,QAAQ,CAACE,IAAI,EAAC;MAEjC,IAAIF,QAAQ,CAACG,EAAE,EAAE;QACbC,OAAO,CAACC,GAAG,CAACP,KAAK,CAACC,IAAI,CAAC;QACvB,IAAI,CAACN,KAAK,CAACK,KAAK,CAACQ,QAAO,GAAIJ,IAAG;MACnC;IACJ;IAAE,IAAIT,KAAK,CAACK,KAAK,CAACC,IAAI,EAAE;MACpBF,OAAO,EAAC;IACZ;IACA;IACA,OAAO;MACHJ,KAAK;MACLC,OAAO;MACPC,OAAO;MACPC;MACA;IACJ;EACJ,CAAC;;EACDW,OAAO,EAAE;IAELC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzBvB,KAAK,CAACwB,MAAM,CAAE,+BAA8BD,EAAG,EAAC,EAAE;QAC9CE,OAAO,EAAE;UAAE,eAAe,EAAG,UAAS,IAAI,CAAClB,KAAK,CAACK,KAAK,CAACc,MAAO;QAAE;MACpE,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;QACX,IAAI,CAACrB,KAAK,CAACsB,QAAQ,CAAC,SAAS;MACjC,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAKb,OAAO,CAACC,GAAG,CAACY,GAAG,CAAC;IACxC,CAAC;IACDC,aAAa,EAAE,UAAUT,EAAE,EAAE;MACzB,IAAI,CAACf,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;MAC3B,IAAI,IAAI,CAACC,OAAM,KAAM,IAAI,EAAE;QACvBT,KAAK,CAACiC,KAAK,CAAE,+BAA8BV,EAAG,EAAC,EAAE;UAC7CW,IAAI,EAAE,IAAI,CAACzB,OAAO;UAClBgB,OAAO,EAAE;YAAE,eAAe,EAAG,UAAS,IAAI,CAAClB,KAAK,CAACK,KAAK,CAACc,MAAO;UAAE;QAEpE,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;UACX,IAAI,CAACrB,KAAK,CAACsB,QAAQ,CAAC,SAAS;UAC7B,IAAI,CAACrB,OAAM,GAAI,KAAI;UACnB,IAAI,CAACC,OAAM,GAAI,IAAG;QACtB,CAAC,EACAqB,KAAK,CAAEC,GAAG,IAAKb,OAAO,CAACC,GAAG,CAACY,GAAG,CAAC,CAAC;MACzC;IACJ,CAAC;IACDI,SAAS,EAA