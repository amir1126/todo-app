{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'signin',\n  setup() {\n    const store = useStore();\n    const error = ref(false);\n    const email = ref('');\n    const password = ref('');\n    const result = ref(false);\n    const isError = ref(false);\n    const submitSiginup = async e => {\n      e.preventDefault();\n      const response = await axios.post('http://localhost:3000/api/signup', {\n        email: this.email,\n        password: this.password\n      });\n      localStorage.setItem('user', JSON.stringify(response.data.token));\n    };\n    return {\n      error,\n      isError,\n      submitSiginup,\n      email,\n      password,\n      result\n    };\n  }\n};","map":{"version":3,"mappings":"AAgCA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIJ,QAAQ,EAAC;IACvB,MAAMK,KAAI,GAAIN,GAAG,CAAC,KAAK;IACvB,MAAMO,KAAI,GAAIP,GAAG,CAAC,EAAE;IACpB,MAAMQ,QAAO,GAAIR,GAAG,CAAC,EAAE;IACvB,MAAMS,MAAK,GAAIT,GAAG,CAAC,KAAK;IACxB,MAAMU,OAAM,GAAIV,GAAG,CAAC,KAAK;IACzB,MAAMW,aAAY,GAAI,MAAOC,CAAC,IAAK;MAC/BA,CAAC,CAACC,cAAc,EAAC;MACjB,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,kCAAkC,EAAE;QAClER,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACnB,CAAC;MACDQ,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACL,QAAQ,CAACM,IAAI,CAACC,KAAK,CAAC;IACpE;IAEA,OAAO;MACHf,KAAK;MACLI,OAAO;MACPC,aAAa;MACbJ,KAAK;MACLC,QAAQ;MACRC;IACJ;EACJ;AACJ","names":["ref","useStore","axios","name","setup","store","error","email","password","result","isError","submitSiginup","e","preventDefault","response","post","localStorage","setItem","JSON","stringify","data","token"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/signin.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit=\"submitSiginup\">\n        <div class=\"container\">\n            <div>\n                <h1>Sign In</h1>\n                <p>Please fill in this form to create an account.</p>\n                <hr>\n\n                <label for=\"email\"><b>Email</b></label>\n                <input class=\"input\" v-model=\"email\" type=\"text\" placeholder=\"Enter Email\" name=\"email\"\n                    required>\n\n                <label for=\"psw\"><b>Password</b></label>\n                <input class=\"input\" v-model=\"password\" type=\"password\" placeholder=\"Enter Password\"\n                    name=\"psw\" required>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"signupbtn\">Sign In</button>\n                </div>\n                {{ store.state.email }} {{ store.state.password }}\n                <div class=\"error\" v-if=\"store.state.isError\">\n                    {{ store.state.error }}\n                </div>\n                <div v-if=\"store.state.result\">\n                    {{ store.state.result }}\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport { useStore } from 'vuex'\nimport axios from 'axios'\nexport default {\n    name: 'signin',\n    setup() {\n        const store = useStore()\n        const error = ref(false)\n        const email = ref('')\n        const password = ref('')\n        const result = ref(false)\n        const isError = ref(false)\n        const submitSiginup = async (e) => {\n            e.preventDefault()\n            const response = await axios.post('http://localhost:3000/api/signup', {\n                email: this.email,\n                password: this.password\n            })\n            localStorage.setItem('user', JSON.stringify(response.data.token))\n        }\n\n        return {\n            error,\n            isError,\n            submitSiginup,\n            email,\n            password,\n            result\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}