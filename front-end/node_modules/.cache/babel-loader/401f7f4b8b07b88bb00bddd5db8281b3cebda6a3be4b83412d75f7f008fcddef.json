{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'signin',\n  setup() {\n    const email = ref('');\n    const password = ref('');\n    const store = useStore();\n    const error = ref(false);\n    const result = ref(false);\n    const isError = ref(false);\n    const submitSiginup = e => {\n      e.preventDefault();\n      axios.post('http://localhost:4000/signin', {\n        email: store.state.email,\n        password: store.state.password\n      }).then(res => {\n        store.state.result = res.data;\n        isError.value = false;\n        store.state.delresult = true;\n        result.value = true;\n        this.$refs.form.reset();\n        localStorage.setItem('user', JSON.stringify(res));\n        console.log(res.data.email);\n      }).catch(err => {\n        isError.value = true;\n        store.state.delresult = false;\n        error.value = err.response.data.msg;\n        console.log(err.response.data.msg);\n      });\n    };\n    return {\n      email,\n      password,\n      error,\n      isError,\n      submitSiginup,\n      store,\n      result\n    };\n  }\n};","map":{"version":3,"mappings":"AA8BA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,MAAMM,QAAO,GAAIN,GAAG,CAAC,EAAE;IACvB,MAAMO,KAAI,GAAIN,QAAQ,EAAC;IACvB,MAAMO,KAAI,GAAIR,GAAG,CAAC,KAAK;IACvB,MAAMS,MAAK,GAAIT,GAAG,CAAC,KAAK;IACxB,MAAMU,OAAM,GAAIV,GAAG,CAAC,KAAK;IACzB,MAAMW,aAAY,GAAKC,CAAC,IAAK;MACzBA,CAAC,CAACC,cAAc,EAAE;MAClBX,KAAK,CAACY,IAAI,CAAC,8BAA8B,EAAE;QACvCT,KAAK,EAAEE,KAAK,CAACQ,KAAK,CAACV,KAAK;QACxBC,QAAQ,EAAEC,KAAK,CAACQ,KAAK,CAACT;MAC1B,CAAC,EACIU,IAAI,CAAEC,GAAG,IAAK;QACXV,KAAK,CAACQ,KAAK,CAACN,MAAK,GAAIQ,GAAG,CAACC,IAAG;QAC5BR,OAAO,CAACS,KAAI,GAAI,KAAI;QACpBZ,KAAK,CAACQ,KAAK,CAACK,SAAQ,GAAI,IAAG;QAC3BX,MAAM,CAACU,KAAI,GAAI,IAAG;QAClB,IAAI,CAACE,KAAK,CAACC,IAAI,CAACC,KAAK,EAAC;Q